{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-multiple-empty-lines":"off",
    
    "prefer-const": "off",
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "brace-style": "off",
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "space-before-function-paren": "off",
    "func-names": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "ts": "never"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
