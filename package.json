{
  "name": "desafio_triyo_tecnologia",
  "version": "1.0.0",
  "main": "startup.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start:prod": "SET NODE_ENV=production&& tsc --project tsconfig.json && node dist/startup.js",
    "start:client": "cd client && ng serve --configuration=production --open",
    "start:all": "npm-run-all --parallel start:prod start:client",
    "dev": "SET NODE_ENV=development&& ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/startup.ts",
    "dev:client": "cd client && yarn serve --open",
    "dev:all": "npm-run-all --parallel dev dev:client",
    "dev:test": "SET NODE_ENV=qas&& ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/startup.ts",
    "test": "mocha --timeout 5000",
    "generatedb": "node src/database/fake_db/db.js"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.0",
    "@types/mongoose": "^5.10.3",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.22.1",
    "mocha": "^8.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13",
    "npm-run-all": "^4.1.5",
    "swagger-ui-express": "^4.1.6"
  }
}
